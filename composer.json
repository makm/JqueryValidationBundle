{
    "name": "boekkooi/jquery-validation-bundle",
    "type": "symfony-bundle",
    "keywords": [ "jquery", "form", "validation", "validator", "symfony", "twig"],
    "description": "Jquery form validation bundle for symfony 2",
    "require": {
        "php": ">=5.5",
        "symfony/framework-bundle": "~3.0|~4.0|~5.0",
        "symfony/dependency-injection": ">=3.0|^4.0|^5.0",
        "symfony/form": "~3.0|~4.0|~5.0",
        "symfony/validator": "~3.0|~4.0|~5.0",
        "symfony/twig-bundle": "~2.3|~3.0|~4.0",
        "symfony/twig-bridge": "~2.3|~3.0|~4.0",
        "twig/twig": ">=1.14.2|~3.0|~4.0|~5.0",
        "doctrine/collections": "~1.0",
        "doctrine/annotations": "^1.12",
        "symfony/security-csrf": "^5.2",
        "symfony/templating": "^5.2"
    },
    "require-dev": {
        "symfony/browser-kit": "~2.3|~3.0",
        "symfony/dom-crawler": "~2.3|~3.0",
        "symfony/finder": "~2.3|~3.0",
        "symfony/yaml": "~2.3|~3.0",
        "symfony/css-selector": "~2.3|~3.0",
        "symfony/phpunit-bridge": "^5.2",

        "behat/behat": "~3.0",
        "behat/mink": "~1.6",
        "behat/mink-selenium2-driver":  "~1.1",
        "behat/mink-extension": "~2.0",
        "behat/symfony2-extension": "~2.0",
        "symfony/var-dumper": "^5.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Warnar Boekkooi",
            "email": "warnar@boekkooi.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Boekkooi\\Bundle\\JqueryValidationBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Boekkooi\\Bundle\\JqueryValidationBundle\\": "tests/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
